{
    "manifestVersion": 1,
    "id": "mirror-git-repository",
    "name": "Mirror Git Repository",
    "version": "0.1.0",
    "publisher": "travis-kosarek",
    "tags": ["git", "clone", "push", "mirror", "--mirror", "repo", "repository"],
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "description": "A straightforward utility to mirror one Git repository to another location",
    "categories": ["Build and release"],
    "icons": { "default": "images/extension-icon.png" },
    "files": [{
        "path": ".release"
    }],
    "content": {
        "details": {
            "path": "OVERVIEW.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "branding": {
        "color": "rgb(255, 255, 255)",
        "theme": "light"
    },
    "links": {
        "home": {
            "uri": "http://swellaby.com"
        },
        "getstarted": {
            "uri": "https://github.com"
        },
        "support": {
            "uri": "https://github.com/issues"
        },
        "repository": {
            "uri": "https://github.com"
        },
        "issues": {
            "uri": "https://github.com/issues"
        },
        "license": {
            "uri": "https://github.com"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com"
    },
    "contributions": [{
        "id": "git-clone-mirror-vsts-task",
        "description": "A straightforward utility to mirror one Git repository to another location",
        "type": "ms.vss-distributed-task.task",
        "targets": ["ms.vss-distributed-task.tasks"],
        "properties": {
            "name": ".release/git-mirror"
        }
    }]
}